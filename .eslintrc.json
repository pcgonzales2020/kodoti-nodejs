{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-use-before-define": "warn",
        "spaced-comment": "warn",
        "indent": [
            "error",
            4
        ],
        "semi": [
            2,
            "always"
        ],
        "space-before-function-paren": "off",
        "one-var": "off",
        "no-useless-constructor": "off",
        "no-unneeded-ternary": "off",
        "no-constant-condition": "off",
        "eol-last": "off",
        "quotes": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    }
}